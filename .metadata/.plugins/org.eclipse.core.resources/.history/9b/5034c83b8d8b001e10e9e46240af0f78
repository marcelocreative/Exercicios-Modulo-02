package recursividade;

public class CaculadoraFatorial {

	private static final int MAX_ELEMENTOS = 100;
	private int 
	
	public static int calculoFatorialComRecursao(Integer num) {
		
		if(num == 0) {
			return 1;
		}
		
		return num * calculoFatorialComRecursao(num-1);
		
	}
	
	public static Integer calculoFatorialComRecursaoEProgramacaoDinamica(Integer num) {
		
		if(num == 0) {
			return 1;
		}
		
		return num * calculoFatorialComRecursao(num-1);
		
	}
	
}
